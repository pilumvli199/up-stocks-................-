# Upstox credentials
UPSTOX_ACCESS_TOKEN=your_upstox_access_token_here

# Telegram (optional)
TELEGRAM_BOT_TOKEN=123456:ABC-DEF...
TELEGRAM_CHAT_ID=@your_channel_or_numeric_chat_id

# Option expiries
OPTION_EXPIRY_NIFTY=2025-10-07
OPTION_EXPIRY_TCS=2025-10-28

# Polling & window
POLL_INTERVAL=60
STRIKE_WINDOW=5

# Instruments & market-quote endpoints (defaults usually ok)
UPSTOX_INSTRUMENTS_JSON_URL=https://assets.upstox.com/market-quote/instruments/exchange/complete.json.gz
UPSTOX_MARKET_QUOTE_URL=https://api.upstox.com/v2/market-quote/quotes

# Rate-limit tuning
MAX_KEYS_PER_POLL=30
BATCH_SIZE=20
BASE_DELAY=2.0
MAX_RETRIES=1

# WebSocket settings
MARKET_FEED_AUTHORIZE_URL=https://api.upstox.com/v2/market-data-feed/authorize
USE_PROTO=False
RECONNECT_BASE_DELAY=1.0
RECONNECT_MAX_DELAY=60.0
DEFAULT_SUBSCRIBE_BATCH=100
WS_READ_TIMEOUT=60
HEARTBEAT_INTERVAL=25
